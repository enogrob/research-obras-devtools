{
	"name": "rails-open-partial",
	"displayName": "Rails Open Partials",
	"description": "open partial file with go to definition",
	"version": "0.0.5",
	"publisher": "shanehofstetter",
	"license": "SEE LICENSE IN LICENSE.md",
	"icon": "images/icon.png",
	"repository": {
		"type": "git",
		"url": "https://github.com/shanehofstetter/rails-open-partial-vscode"
	},
	"engines": {
		"vscode": "^1.15.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:haml",
		"onLanguage:erb"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": []
	},
	"scripts": {
		"publish": "vsce publish",
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && mocha ./test/extension.test.js"
	},
	"devDependencies": {
		"typescript": "^2.5.2",
		"vscode": "^1.1.37",
		"mocha": "^10.2.0",
		"@types/node": "^7.0.43",
		"@types/mocha": "^2.2.42"
	},
